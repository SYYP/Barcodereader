<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="120dp"
    android:layout_height="140dp"
    android:id="@+id/pop_window"
    android:background="@drawable/pop_winwod">

    <TextView

        android:drawablePadding="10dp"
        android:text="导出Excel"
        android:drawableLeft="@drawable/img_daochu"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true"
        android:layout_marginStart="11dp"
        android:layout_marginTop="35dp"
        android:id="@+id/text_excel" />
    <View
        android:id="@+id/view_ones"
        android:layout_marginTop="10dp"
        android:layout_below="@id/text_excel"
        android:background="@color/linecolor"
        android:layout_width="match_parent"
        android:layout_height="1dp"></View>
    <TextView
        android:layout_marginTop="10dp"
        android:layout_marginStart="11dp"
        android:layout_below="@id/view_ones"
        android:drawablePadding="10dp"
        android:text="分享"
        android:drawableLeft="@drawable/img_share"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/text_share" />
    <View
        android:id="@+id/view_twos"
        android:layout_marginTop="10dp"
        android:layout_below="@id/text_share"
        android:background="@color/linecolor"
        android:layout_width="match_parent"
        android:layout_height="1dp"></View>
    <TextView
        android:layout_marginTop="10dp"
        android:layout_marginStart="11dp"
        android:layout_below="@id/view_twos"
        android:drawablePadding="10dp"
        android:text="删除"
        android:drawableLeft="@drawable/img_delete"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/text_delete" />
</RelativeLayout>
